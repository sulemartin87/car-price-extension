{
  "name": "Car price extension",
  "version": "1.0",
  "description": "Get current car prices on certain sites",
  "permissions": ["activeTab"],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/car_price_16.png",
      "32": "images/car_price_32.png",
      "48": "images/car_price_48.png",
      "128": "images/car_price_128.png"
    }
  },
  "icons": {
      "16": "images/car_price_16.png",
      "32": "images/car_price_32.png",
      "48": "images/car_price_48.png",
      "128": "images/car_price_128.png"
  },
  "manifest_version": 2
}
